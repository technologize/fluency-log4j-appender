<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>io.github.technologize</groupId>
	<artifactId>fluency-log4j-appender-aggreator</artifactId>
	<version>1.0.0</version>
	<packaging>pom</packaging>
	<name>Fluency Log4j Appender Aggregator</name>
	<description>Aggregator for Log4j Appender Based on Fluency</description>
	<url>https://github.com/technologize/fluency-log4j-appender</url>

	<modules>
		<module>fluency-log4j-appender-core</module>
		<module>fluentd-log4j-appender</module>
		<module>aws-s3-log4j-appender</module>
	</modules>

	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-deploy-plugin</artifactId>
				<version>3.0.0-M2</version>
				<configuration>
					<skip>true</skip>
				</configuration>
			</plugin>
		</plugins>
		
	</build>
</project>